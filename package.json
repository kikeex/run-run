{
  "name": "run-run",
  "version": "0.0.1",
  "description": "Backend for run-run",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon /src/sever.js",
    "test": "jest",
    "commit": "git-cz",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@sendgrid/mail": "^7.1.0",
    "body-parser": "^1.19.0",
    "commitizen": "^3.0.7",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "husky": "^1.3.1",
    "jest": "^24.9.0",
    "lint-staged": "^8.1.5",
    "supertest": "^4.0.2",
    "winston": "^3.2.1"
  },
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ],
    "globals": [
      "__DEV__",
      "fetch",
      "URL",
      "Event",
      "Date"
    ]
  },
  "devDependencies": {
    "eslint": "^6.3.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-strongloop": "^2.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "nodemon": "^1.19.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "linters": {
      "**/*.js": [
        "npm run lint",
        "git add"
      ]
    }
  },
  "author": "Kikeex"
}
